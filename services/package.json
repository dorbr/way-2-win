{
  "name": "financial-dashboard",
  "version": "1.0.0",
  "description": "Daily financial and macroeconomic dashboard",
  "main": "server.js",
  "scripts": {
    "start": "ts-node src/server.ts",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "npx prisma generate && tsc",
    "start:prod": "node dist/server.js",
    "update-options": "node dist/scripts/run-daily-options.js",
    "clear-db": "ts-node src/scripts/clear-db.ts",
    "seed-tickers": "ts-node src/scripts/seed-tickers.ts",
    "verify-data": "ts-node src/scripts/verify-data.ts"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@polygon.io/client-js": "^8.2.0",
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.0",
    "express": "^4.18.0",
    "openai": "^6.9.1",
    "pg": "^8.16.3",
    "yahoo-finance2": "^3.10.2"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "autoprefixer": "^10.4.22",
    "postcss": "^8.5.6",
    "prisma": "^7.0.1",
    "tailwindcss": "^4.1.17",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
